<?xml version="1.0"?>
<launch>
  <param name="use_sim_time" value="true"/>

  <node pkg="stage_ros" type="stageros" name="stageros" args="-d $(find stage_simulation)/worlds/rrtdmp_4.world">
    <!--remap from="tf" to="tf_old"/-->
    <remap from="/robot_0/cmd_vel" to="/robot_0/cmd_vel_0"/>
  </node>

  <group ns="01">
  <node pkg="rosbag" type="play" name="bag_01"
    args="--clock /home/mark/bagfiles/rrtdmp_01.bag"/>
  </group>
  <group ns="02">
  <node pkg="rosbag" type="play" name="bag_02"
    args="--clock /home/mark/bagfiles/rrtdmp_02.bag"/>
  </group>
  <group ns="03">
  <node pkg="rosbag" type="play" name="bag_03"
    args="--clock /home/mark/bagfiles/rrtdmp_03.bag"/>
  </group>
  <group ns="04">
  <node pkg="rosbag" type="play" name="bag_04"
    args="--clock /home/mark/bagfiles/rrtdmp_04.bag"/>
  </group>
  <group ns="05">
  <node pkg="rosbag" type="play" name="bag_05"
    args="--clock /home/mark/bagfiles/rrtdmp_05.bag"/>
  </group>
  <group ns="06">
  <node pkg="rosbag" type="play" name="bag_06"
    args="--clock /home/mark/bagfiles/rrtdmp_06.bag"/>
  </group>
  <group ns="07">
  <node pkg="rosbag" type="play" name="bag_07"
    args="--clock /home/mark/bagfiles/rrtdmp_07.bag"/>
  </group>
  <group ns="08">
  <node pkg="rosbag" type="play" name="bag_08"
    args="--clock /home/mark/bagfiles/rrtdmp_08.bag"/>
  </group>
  <group ns="09">
  <node pkg="rosbag" type="play" name="bag_09"
    args="--clock /home/mark/bagfiles/rrtdmp_09.bag"/>
  </group>
  <group ns="10">
  <node pkg="rosbag" type="play" name="bag_10"
    args="--clock /home/mark/bagfiles/rrtdmp_10.bag"/>
  </group>

  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find planners)/launch/config/rviz/rrtdmp_bag.rviz">
    <remap from="initialpose" to="robot_0/initialpose"/><!--remap rviz initial position button-->
    <remap from="move_base_simple/goal" to="robot_0/move_base_simple/goal"/><!--remap rviz goal position button-->
  </node>

  <node pkg="map_server" name="map_server" type="map_server" args="$(find stage_simulation)/worlds/maps/rrtdmp_4.yaml">
    <!--remap from="/map" to="map"/-->
  </node>
</launch>
