<?xml version="1.0"?>

<launch>
  <arg name="model"/>
  <arg name="gui" default="False"/>
  <param name="use_sim_time" value="true"/>
  <param name="robot_description" command="cat $(find robot_odometry)/URDF/rangerX.urdf"/>
  <!--node pkg="tf" type="static_transform_publisher" name="link1_broadcaster" args="0 0 1.2 4.71 0 0 /base_footprint /laser_frame 33"/-->
  <!--node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    <param name="publish_frequency" type="double" value="10.0"/>
  </node-->
  <!--node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher">
    <param name="use_gui" value="true"/>
  </node-->

  <node pkg="rviz" type="rviz" name="rviz"/>
    
  <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
    <!--param name="maxUrange" value="5.5"/>
    <param name="maxRange" value="5.5"/>
    <param name="minimumScore" value="50"/>
    <param name="linearUpdate" value="0.2"/>
    <param name="angularUpdate" value="0.25"/>
    <param name="temporalUpdate" value="-1.0"/>
    <param name="map_update_interval" value="2.0"/>
    <param name="delta" value="0.025"/-->
    <param name="base_frame" value="base_footprint"/>
    <param name="map_frame" value="map"/>
    <param name="odom_frame" value="odom"/>
    <param name="xmin" value="-50.0"/>
    <param name="ymin" value="-50.0"/>
    <param name="xmax" value="50.0"/>
    <param name="ymax" value="50.0"/>
  </node>
  

  <node pkg="rosbag" type="play" name="player" args="--clock /home/mark/laserData6.bag"/>

</launch>
