<?xml version="1.0"?>
<launch><master auto="start"/>
  <param name="use_sim_time" value="false"/>

  <remap from="scan" to="base_scan"/>

  <!--Calibration Parameters-->
  <param name="base_width" type="double" value="0.56"/><!--Robot Base Width-->
  <param name="dcal_L" type="double" value="1.05"/><!--Left Wheel m/rev-->
  <param name="dcal_R" type="double" value="1.05"/><!--Right Wheel m/rev-->
  <param name="ecal_L" type="double" value="4863.0"/><!--Left Encoder C/rev-->
  <param name="ecal_R" type="double" value="4848.0"/><!--Right Encoder C/rev-->
  <param name="KPA" type="double" value="0.03"/><!--Angular Proportional Gain-->
  <param name="KDA" type="double" value="0.3"/><!--Angular Derivative Gain-->
  <param name="KIA" type="double" value="0.00005"/><!--Angular Integral Gain-->
  <param name="KPL" type="double" value="0.1"/><!--Linear Proportional Gain-->
  <param name="KDL" type="double" value="0.001"/><!--Linear Derivative Gain-->

  <arg name="model"/>
  <arg name="gui" default="False"/>
  <param name="robot_description" command="cat $(find robot_odometry)/URDF/rangerX.urdf"/>

  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    <param name="publish_frequency" value="10.0"/>
  </node>
  <node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher">
    <param name="use_gui" value="true"/>
  </node>

  <node pkg="robot_odometry" type="rangerx_odom" name="rangerX_odom" required="false" output="screen"/>

  <node pkg="rosserial_arduino" type="serial_node.py" name="serial_node">
    <param name="~port" value="/dev/ttyACM0"/>
  </node>

  <node pkg="rplidar_ros"  type="rplidarNode" name="rplidarNode">
    <param name="serial_port"         type="string" value="/dev/ttyUSB0"/>
    <param name="serial_baudrate"     type="int"    value="256000"/>
    <param name="frame_id"            type="string" value="laser"/>
    <param name="inverted"            type="bool"   value="false"/>
    <param name="angle_compensate"    type="bool"   value="true"/>
    <param name="scan_mode"           type="string" value="Standard"/>
  </node>

  <node pkg="amcl" type="amcl" name="amcl">
    <param name="use_map_topic" value="true"/>
    <param name="base_frame_id" value="base_footprint"/>
    <param name="tf_broadcast" value="true"/>
    <param name="odom_model_type" value="diff-corrected"/>
    <param name="odom_alpha1" value="0.2"/>
    <param name="odom_alpha2" value="0.2"/>
    <param name="odom_alpha3" value="0.2"/>
    <param name="odom_alpha4" value="0.2"/>
  </node>

  <!--include file="$(find amcl)/examples/amcl_diff.launch" /-->

  <!--node pkg="costmap_2d" type="costmap_2d_node" name="costmap_2d_node">
    <rosparam file="$(find robot_navigation)/launch/costmap_common_params.yaml" ns="costmap"/>
    <rosparam file="$(find robot_navigation)/launch/local_costmap_params.yaml" ns="costmap"/>
  </node-->

  <node pkg="move_base" type="move_base" name="move_base" output="screen">
    <rosparam file="$(find robot_navigation)/launch/rangerx/costmap_common_params.yaml" command="load" ns="global_costmap"/>
    <rosparam file="$(find robot_navigation)/launch/rangerx/costmap_common_params.yaml" command="load" ns="local_costmap"/>
    <rosparam file="$(find robot_navigation)/launch/rangerx/local_costmap_params.yaml" command="load"/>
    <rosparam file="$(find robot_navigation)/launch/rangerx/global_costmap_params.yaml" command="load"/>
    <rosparam file="$(find robot_navigation)/launch/rangerx/base_local_planner_params.yaml" command="load"/>
    <param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS"/>
    <param name="recovery_behavior_enabled" value="false"/>
    <param name="controller_frequency" value="10.0"/>
    <param name="use_dijkstra" value="false"/>
    <param name="visualize_potential" value="true"/>
  </node>

  <node pkg="map_server" name="map_server" type="map_server" args="$(find robot_navigation)/maps/fsde-firstfloor.pgm 0.050000"/>

  <!--node pkg="gmapping" type="slam_gmapping" name="slam_gmapping">
    <param name="base_frame" value="base_footprint"/>
    <param name="map_frame" value="map"/>
    <param name="odom_frame" value="odom"/>
    <param name="xmin" value="-50.0"/>
    <param name="ymin" value="-50.0"/>
    <param name="xmax" value="50.0"/>
    <param name="ymax" value="50.0"/>
    <param name="temporalUpdate" value="1.0"/>
  </node-->

  <node pkg="rviz" type="rviz" name="rviz"/>
</launch>
